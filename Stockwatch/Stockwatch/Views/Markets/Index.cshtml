@model IEnumerable<Domain.Market>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MarketIndex)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MarketDescription)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MarketIndex)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MarketDescription)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.MarketId }) |
            @Html.ActionLink("Details", "Details", new { id=item.MarketId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.MarketId })
        </td>
    </tr>
}

</table>
